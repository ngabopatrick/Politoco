  "bugs": {
    "url": "https://github.com/ngabopatrick/Politico/issues"
  },
  "homepage": "https://github.com/ngabopatrick/Politico#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "bcrypt": "^3.0.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "custom-env": "^1.0.0",
    "dotenv": "^6.2.0",
    "eslint": "^5.13.0",
    "express": "^4.17.3",
    "joi": "^14.3.1",
    "joi-phone-number": "^2.1.1",
    "jsonwebtoken": "^8.5.0",
    "make-runnable": "^1.3.6",
    "moment": "^2.24.0",
    "nodemon": "^1.18.10",
    "pg": "^7.8.0",
    "pg-hstore": "^2.3.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "chai-things": "^0.2.0",
    "coveralls": "^3.0.2",
    "mocha": "^5.2.0",
    "nyc": "^13.2.0"
  }
